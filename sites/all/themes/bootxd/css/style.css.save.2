.html{
        background-color:#ffffff;
}
body{
	padding: 4px;
}
#navbar{
	padding: 0px;
	-webkit-box-shadow: 0px 0px 7px 6px rgba(51,121,183,20);
	-moz-box-shadow: 0px 0px 7px 6px rgba(51,121,183,20);
	box-shadow: 0px 0px 7px 6px rgba(51,121,183,20);
}
.navbar-brand{
	font-size: 24px;
}
.menu.nav.navbar-nav.secondary{
        float:right;
}
@media (max-width: 767px) {
        .menu.nav.navbar-nav.secondary{ float:left; } 
 }
.navbar-default.navbar-nav>li>a{
	color: #fff;
}

.thumbnail{
	text-align:center;
	height:265px;
	overflow:hidden;
	position: relative;
	background-color: #fff;
	-webkit-background-clip: padding-box;
        	 background-clip: padding-box;
	border: 1px solid #999;
	border: 1px solid rgba(0, 0, 0, .2);
	border-radius: 6px;
	outline: 0;

	/*Shadow effect-1*/
	-webkit-box-shadow: 0 3px 9px rgba(0, 0, 0, .5);
          box-shadow: 0 3px 9px rgba(0, 0, 0, .5);
         -moz-box-shadow: 0 0 10px #ccc;

	/*Shadow effect -2*/
/*	-webkit-box-shadow: 0px 0px 1px 0px rgba(0,0,0,0.75);
	-moz-box-shadow: 0px 0px 1px 0px rgba(0,0,0,0.75);
	box-shadow: 0px 0px 1px 0px rgba(0,0,0,0.75);
*/
}
.thumbnail:hover {
	color: #3a7999;
/*Transtition effect*/
	box-shadow: inset 0 0 0 3px #3a7999;
	transform:scale(1.1);
	transition: all 0.3s ease 0s;
	width:100%;

	/*Shadow effect*/
/*	-webkit-box-shadow: 0px 7px 11px 0px rgba(0,0,0,0.75);
	-moz-box-shadow: 0px 7px 11px 0px rgba(0,0,0,0.75);
	box-shadow: 0px 7px 11px 0px rgba(0,0,0,0.75);
*/
}
